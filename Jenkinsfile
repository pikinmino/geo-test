pipeline{
    agent any
    stages{
    stage('maven clean'){
        steps {
            echo 'mvn clean'
            }
    }
    stage('maven install'){
        steps{
            echo 'mvn install'
        }
    }
    stage('maven package'){
        steps{
            echo 'mvn package'
        }
    }
        
            
   }
 }
